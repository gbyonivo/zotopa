{
  "name": "zotopa",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "node node_modules/.bin/jest",
    "start": "webpack-dev-server --mode development --open",
    "build": "webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gbyonivo/zotopa.git"
  },
  "author": "Orkuma Ivo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gbyonivo/zotopa/issues"
  },
  "homepage": "https://github.com/gbyonivo/zotopa#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.1.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-css-modules": "^2.7.5",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.4.1",
    "node-sass": "^4.9.2",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.4.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "date-fns": "^1.29.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "firebase-client": "^0.1.1",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-geolocated": "^2.4.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "styled-components": "^3.3.3",
    "webfontloader": "^1.6.28"
  },
  "jest": {
    "bail": true,
    "notify": true,
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.scss$": "identity-obj-proxy"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/api/**/*.js",
      "src/actions/**/*.js",
      "src/components/**/*.js",
      "src/functions/**/*.js",
      "src/hocs/**/*.js",
      "src/reducers/**/*.js",
      "src/sagas/**/*.js",
      "src/selectors/**/*.js"
    ],
    "coverageReporters": [
      "text",
      "text-summary",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 80,
        "branches": 80,
        "functions": 80,
        "lines": 80
      }
    },
    "verbose": false,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "<rootDir>/src/setupTests.js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|webm)$": "<rootDir>/jestFileTransformer.js"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/babel-polyfill/"
    ]
  }
}
